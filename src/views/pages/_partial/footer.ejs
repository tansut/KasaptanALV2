                    <h3 class="widget-title text-light">Ürünler</h3>
                    <ul class="widget-list">
                        <% var viewCats = ['reyon', 'list'] %>
                        <% viewCats.forEach(cat => { %>
                        <% var reyonlar = __categories.filter(p=>p.type.includes(cat) && p.status == 'active') %>
                        <% for(var c=0; c< reyonlar.length; c++) {%>
                        <li class="widget-list-item"><a class="widget-list-link"
                                href="<%= `/${reyonlar[c].slug}`%>"><%= reyonlar[c].name %></a></li>
                        <%}%>
                        <% }) %>
                        <li class="widget-list-item">
                            <a class="widget-list-link" href="<%= `/kasap-urunleri`%>"><i
                                    class="czi-arrow-right-circle mr-2"></i>Tümü</a>
                        </li>

                    </ul>
                </div>                
                <div class="widget widget-links widget-light pb-2 mb-4">
                    <h3 class="widget-title text-light">Et Yemekleri</h3>
                    <ul class="widget-list">
                        <% var viewCats = ['resource'] %>
                        <% viewCats.forEach(cat => { %>
                        <% var reyonlar = __categories.filter(p=>p.type == cat && p.status == 'active') %>
                        <% for(var c=0; c< reyonlar.length; c++) {%>
                        <li class="widget-list-item"><a class="widget-list-link"
                                href="/et-yemekleri/<%= `${reyonlar[c].slug}`%>"><%= reyonlar[c].name %></a></li>
                        <%}%>
                        <% }) %>
                        <li class="widget-list-item">
