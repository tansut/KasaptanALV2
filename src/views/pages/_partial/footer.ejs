                </div> 
                <div class="widget widget-links widget-light pb-2 mb-4 mr-4">
                    <h3 class="widget-title text-light">Online Kasaplar</h3>
                    <% var cityChunks = _.chunk(__butcherCities, 12) %>
                    <div class="row">
                        <% cityChunks.forEach((chunk, j) => { %>
                        <div class="col">
                            <ul class="widget-list">
                                <% for(var i=0; i<chunk.length; i++) {%>
                                    <% if (chunk[i].name == 'Ankara' || chunk[i].name == 'Istanbul') {%>
                                <li class="widget-list-item"><a class="widget-list-link"
                                        href="<%= `/${chunk[i].slug}-kasap` %>"><%= chunk[i].name %></a></li>
                                <%}%>

                                <%}%>
                                    
                                    <% if (j == 0) { %>
                                <li class="widget-list-item">
                                    <a class="widget-list-link" href="<%= `/onlinekasap`%>"><i
                                            class="czi-arrow-right-circle mr-2"></i>Tümü</a>
                                </li>
                                <%}%>
                                        
                                                            </ul>                        
                                            </div>
                                            <% }) %>
                        </div>


                    </div>
    
                    
