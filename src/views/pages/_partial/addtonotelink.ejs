<% if (typeof(forceLink) == 'undefined' && resource.otherProducts.length > 0) { %>
<% 

var res = [resource.product].concat(resource.otherProducts); 
var alternatives = [];

%><% res.forEach(p=> { %>

    <% var baseUrl = `/${p.slug}/${subUrl}/${__helper.slugify(resource.title || 'tarif')}?r=${resource.id}&autoNote=1&hideproduct=1`  %>
    <% if (typeof(disableSlider) != 'undefined') baseUrl += '&disableSlider=1' %>

    <% alternatives.push({ 
        src:  '/'+ p.slug + '/resimler/thumbnail' , 
        name:  p.name ,
        link: baseUrl
      }) %>

<% }) %>javascript:window.App.FoodAlternativesApp.show(<%=JSON.stringify(alternatives)%>)
<%} else {%>
    <% var baseUrl = `/${resource.product.slug}/${subUrl}/${__helper.slugify(resource.title || 'tarif')}?r=${resource.id}&autoNote=1`  %>
    <% if (typeof(disableSlider) != 'undefined') baseUrl += '&disableSlider=1' %>
    <%=baseUrl %>
<%}%>